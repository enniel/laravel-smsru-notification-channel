{
    "name": "enniel/laravel-smsru-notification-channel",
    "description": "SmsRu Notifications Driver",
    "homepage": "https://github.com/enniel/laravel-smsru-notification-channel",
    "license": "MIT",
    "authors": [
        {
            "name": "Evgeni Razumov (Enniel)",
            "email": "razumov.evgeni@gmail.com",
            "homepage": "https://github.com/enniel"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/notifications": "^5.3",
        "illuminate/support": "^5.1|^5.2|^5.3",
        "zelenin/smsru": "~4.1.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\SmsRu\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\SmsRu\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
